    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "dataset.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ais_dataset.py",
            "env": {
                "CUDA_VISIBLE_DEVICES": "0,1,2,3"
            },
            "console": "integratedTerminal",
            "args": []
        },
        {
            "name": "main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "env": {
                "CUDA_VISIBLE_DEVICES": "0,1,2,3"
            },
            "console": "integratedTerminal",
            "args": []
        },
        {
            "name": "moment.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main_moment.py",
            "env": {
                "CUDA_VISIBLE_DEVICES": "0,1,2,3"
            },
            "console": "integratedTerminal",
            "args": []
        }
    ]
}